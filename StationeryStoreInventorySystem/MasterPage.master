<%@ Master Language="C#" AutoEventWireup="true" CodeFile="MasterPage.master.cs" Inherits="MasterPage" %>

<!DOCTYPE html>

<html xmlns="http://www.w3.org/1999/xhtml">
<head runat="server">
    <title></title>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="Content/StyleSheet.css" rel="stylesheet" />
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.6/jquery.min.js" type="text/javascript"></script>
    <script src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8/jquery-ui.min.js" type="text/javascript"></script>
    <link href="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8/themes/base/jquery-ui.css" rel="Stylesheet" type="text/css" />
    <script type="text/javascript">
        $(function () {
            $("[id$=txtDate]").datepicker();
        });
    </script>
    <asp:ContentPlaceHolder ID="head" runat="server">
    </asp:ContentPlaceHolder>
</head>

<body>
    <form id="form2" runat="server">
        <div class="wrapper">
            <div class="header row">
                <asp:HyperLink ID="HyperLink6" runat="server" NavigateUrl="~/Main.aspx">
                    <asp:Image ID="Image2" runat="server" ImageUrl="~/images/logo.jpeg" CssClass="logo" />
                </asp:HyperLink>
                <div class="col-s">
                    <asp:Button ID="btnSignOut" Style="width: 28%" runat="server" Text="Sign Out" class="login-btn" OnClick="btnSignOut_Click" />
                </div>
            </div>

            <div class="content row">
                <!-- Menu For Department Head -->
                <div class="left-col" id="DepHeadMenu" runat="server">
                    <asp:HyperLink ID="HyperLink1" runat="server" CssClass="hyperLinkCategoryStyle" NavigateUrl="~/Department/RequisitionListDepartment.aspx">Requisition List</asp:HyperLink>                  
                    <asp:HyperLink ID="HyperLink2" runat="server" CssClass="hyperLinkCategoryStyle" NavigateUrl="~/Department/StationeryCatalogue.aspx">Requisition Form</asp:HyperLink>
                     <asp:HyperLink ID="HyperLink3" runat="server" CssClass="hyperLinkCategoryStyle" NavigateUrl="~/Department/DepartmentListDHead.aspx">Requisition Form</asp:HyperLink>
                </div>

                <!-- Menu For Departement Representative -->
                <div class="left-col" id="DepRepMenu" runat="server">
                </div>

                <!-- Menu For Department Member -->
                <div class="left-col" id="DepMember" runat="server">
                </div>

                <!-- Menu For Store Manager -->
                <div class="left-col" id="StoreManMenu" runat="server">
                    <asp:HyperLink ID="ReorderTrendManager" runat="server" CssClass="hyperLinkCategoryStyle" NavigateUrl="~/TrendReport.aspx?type=ROR">ReOrder Trend Report</asp:HyperLink>           
                    <asp:HyperLink ID="RequisitionTrendManager" runat="server" CssClass="hyperLinkCategoryStyle" NavigateUrl="~/TrendReport.aspx?type=RTR">Requisition Trend Report</asp:HyperLink>     
                    <asp:HyperLink ID="SupplierListManager" runat="server" CssClass="hyperLinkCategoryStyle" NavigateUrl="~/SupplierList.aspx">Supplier List</asp:HyperLink>     
                    <asp:HyperLink ID="SupplierCreateManager" runat="server" CssClass="hyperLinkCategoryStyle" NavigateUrl="~/SupplierCreate.aspx">Create Supplier</asp:HyperLink>     
                </div>

                <!-- Menu For Store Supervisor -->
                <div class="left-col" id="StoreSuperMenu" runat="server">
                    <asp:HyperLink ID="ReorderTrendSupervisor" runat="server" CssClass="hyperLinkCategoryStyle" NavigateUrl="~/TrendReport.aspx?type=ROR">ReOrder Trend Report</asp:HyperLink>           
                    <asp:HyperLink ID="RequisitionTrendSupervisor" runat="server" CssClass="hyperLinkCategoryStyle" NavigateUrl="~/TrendReport.aspx?type=RTR">Requisition Trend Report</asp:HyperLink>     
                    <asp:HyperLink ID="SupplierListSupervisor" runat="server" CssClass="hyperLinkCategoryStyle" NavigateUrl="~/SupplierList.aspx">Supplier List</asp:HyperLink>     
                    <asp:HyperLink ID="SupplierCreateSupervisor" runat="server" CssClass="hyperLinkCategoryStyle" NavigateUrl="~/SupplierCreate.aspx">Create Supplier</asp:HyperLink>     
                </div>


                <!-- Menu For Store Clerk -->
                <div class="left-col" id="StoreClerkMenu" runat="server">
                    <asp:HyperLink ID="ReorderTrendClerk" runat="server" CssClass="hyperLinkCategoryStyle" NavigateUrl="~/TrendReport.aspx?type=ROR">ReOrder Trend Report</asp:HyperLink>           
                    <asp:HyperLink ID="RequisitionTrendClerk" runat="server" CssClass="hyperLinkCategoryStyle" NavigateUrl="~/TrendReport.aspx?type=RTR">Requisition Trend Report</asp:HyperLink>     
                    <asp:HyperLink ID="SupplierListClerk" runat="server" CssClass="hyperLinkCategoryStyle" NavigateUrl="~/SupplierList.aspx">Supplier List</asp:HyperLink>     
                </div>

                <div class="right-col">
                    <div class="contentMain">
                        <asp:ContentPlaceHolder ID="ContentPlaceHolder1" runat="server">
                        </asp:ContentPlaceHolder>
                    </div>
                </div>
            </div>
        </div>
    </form>
</body>
</html>
